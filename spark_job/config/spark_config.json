{
    "spark_config": {
        "spark.master": "local[*]",
        "spark.submit.deployMode": "client",
        "spark.driver.memory": "4g",
        "spark.driver.maxResultSize": "4g",
        "spark.executor.memory": "4g",
        "spark.executor.cores": "2",
        "spark.executor.memoryOverhead": "4g",
        "spark.memory.offHeap.enabled": "true",
        "spark.memory.offHeap.size": "4g",
        "spark.memory.fraction": "0.6",
        "spark.memory.storageFraction": "0.3",
        "spark.default.parallelism": "8",
        "spark.sql.shuffle.partitions": "100",
        "spark.sql.adaptive.enabled": "true",
        "spark.sql.adaptive.coalescePartitions.enabled": "true",
        "spark.sql.adaptive.skewJoin.enabled": "true",
        "spark.sql.adaptive.advisoryPartitionSizeInBytes": "64MB",
        "spark.sql.autoBroadcastJoinThreshold": "-1",
        "spark.sql.catalogImplementation": "hive",
        "spark.sql.hive.metastore.version": "2.3.9",
        "spark.sql.hive.metastore.jars": "builtin",
        "spark.sql.extensions": "io.delta.sql.DeltaSparkSessionExtension",
        "spark.sql.catalog.spark_catalog": "org.apache.spark.sql.delta.catalog.DeltaCatalog",
        "spark.databricks.delta.autoOptimize.optimizeWrite": "true",
        "spark.databricks.delta.autoOptimize.autoCompact": "true",
        "spark.databricks.delta.optimizeWrite.enabled": "true",
        "spark.databricks.delta.autoCompact.enabled": "true",
        "spark.serializer": "org.apache.spark.serializer.KryoSerializer",
        "spark.kryoserializer.buffer": "64m",
        "spark.kryoserializer.buffer.max": "256m",
        "spark.network.timeout": "600s",
        "spark.executor.heartbeatInterval": "30s",
        "spark.shuffle.io.retryWait": "60s",
        "spark.shuffle.io.maxRetries": "10",
        "spark.speculation": "true",
        "spark.task.maxFailures": "4",
        "spark.executor.extraJavaOptions": "-XX:+ExitOnOutOfMemoryError -XX:+UseG1GC -XX:InitiatingHeapOccupancyPercent=35 -Dlog4j.configurationFile=file:/opt/airflow/dags/configurations/log4j2.properties -DLOG_LEVEL=WARN",
        "spark.driver.extraJavaOptions": "-XX:+ExitOnOutOfMemoryError -XX:+UseG1GC -XX:InitiatingHeapOccupancyPercent=35 -Dlog4j.configurationFile=file:/opt/airflow/dags/configurations/log4j2.properties -DLOG_LEVEL=WARN",
        "spark.sql.parquet.mergeSchema": "false",
        "spark.sql.sources.partitionOverwriteMode": "dynamic",
        "spark.sql.orc.filterPushdown": "true",
        "spark.sql.orc.impl": "native",
        "spark.sql.orc.enableVectorizedReader": "true",
        "spark.sql.legacy.timeParserPolicy": "LEGACY",
        "spark.scheduler.mode": "FAIR",
        "spark.ui.enabled": "true"
        
        
    }
}